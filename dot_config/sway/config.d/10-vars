##
# General variables
##

set $mod Mod4
# Home row direction keys, like vim
set $left h
set $down j
set $up k
set $right l

##
# Application variables
##

set $term foot
set $menu wofi --insensitive --matching=fuzzy --allow-images --show=drun

# Theme variables
include "$HOME/.cache/matugen/colors-sway"

